{
  "name": "awoken-bible-reference",
  "version": "1.4.0",
  "description": "Bible verse reference parser, generator and manipulator",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha test/ --reporter spec",
    "cover": "./node_modules/.bin/nyc ./node_modules/.bin/mocha && ./node_modules/.bin/nyc report --reporter lcov --reporter html",
    "build": "tsc",
    "docs": "./node_modules/.bin/typedoc --out docs",
    "prepare": "git clean -Xf && npm run build"
  },
  "dependencies": {
    "parsimmon": "1.13.0"
  },
  "devDependencies": {
    "@types/parsimmon": "^1.10.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "rewire": "^5.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jnterry/awoken-reference.git"
  },
  "keywords": [
    "bible",
    "usfm",
    "parser",
    "format"
  ],
  "author": "Jamie Terry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnterry/awoken-bible-reference/issues"
  },
  "homepage": "https://github.com/jnterry/awoken-bible-reference#readme",
  "files": [
    "dist/**/*"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*/*.d.ts"
    ],
    "all": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  }
}
