{
  "name": "awoken-bible-reference",
  "version": "2.3.0",
  "description": "Bible verse reference parser, generator and manipulator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha test/ -r ts-node/register --reporter spec",
    "cover": "./node_modules/.bin/nyc ./node_modules/.bin/mocha && ./node_modules/.bin/nyc report --reporter lcov --reporter html",
    "build:node": "tsc",
    "build:browser": "webpack --mode=production",
    "build": "npm run build:node && npm run build:browser",
    "docs": "./node_modules/.bin/typedoc --out docs --mode file --excludeNotDocumented --excludePrivate",
    "prepare": "git clean -Xf && npm run build"
  },
  "dependencies": {
    "parsimmon": "^1.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^14.14.22",
    "@types/parsimmon": "^1.10.6",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "source-map-loader": "^2.0.1",
    "source-map-support": "^0.5.19",
    "ts-loader": "^9.1.2",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/awoken-bible/reference.git"
  },
  "keywords": [
    "bible",
    "usfm",
    "parser",
    "format"
  ],
  "author": "Jamie Terry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnterry/awoken-bible-reference/issues"
  },
  "homepage": "https://awoken-bible.github.io/reference/index.html",
  "files": [
    "dist/**/*",
    "dist.browser/**/*"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*/*.d.ts"
    ],
    "all": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "browserslist": "> 0.25%, not dead"
}
